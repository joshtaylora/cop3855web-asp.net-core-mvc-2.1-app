@model IEnumerable<string>

@foreach (string category in Model)
{
    <a class="btn btn-block 
       @(category == ViewBag.SelectedCategory ? "btn-primary": "btn-default")"
       asp-action="Merch"
       asp-controller="Product"
       asp-route-category="@category"
       asp-route-page="1">
        @category
    </a>
}